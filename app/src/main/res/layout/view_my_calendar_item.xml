<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                     xmlns:app="http://schemas.android.com/apk/res-auto"
                                                     xmlns:tools="http://schemas.android.com/tools"
                                                     android:id="@+id/cellDateLayout"
                                                     android:layout_width="match_parent"
                                                     android:layout_height="wrap_content"
                                                     android:elevation="@dimen/elevation_all">

    <TextView
            android:id="@+id/itemDate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="3dp"
            android:layout_marginTop="2dp"
            android:text="1"
            android:textSize="12sp"
            android:textColor="@color/colorMyType"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
    <ImageView
            android:layout_width="144dp"
            android:layout_height="40dp" app:srcCompat="@drawable/icon_my_type" android:id="@+id/date_record"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/date_puzzle" app:layout_constraintHorizontal_bias="0.5"
            android:layout_marginEnd="32dp"
            android:elevation="@dimen/elevation_all" android:layout_marginTop="16dp"
            android:layout_marginBottom="16dp"/>
    <ImageView
            android:layout_width="124dp"
            android:layout_height="42dp" app:srcCompat="@drawable/icon_puzzle" android:id="@+id/date_puzzle"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toStartOf="@+id/date_record"
            android:layout_marginEnd="8dp" app:layout_constraintTop_toTopOf="@+id/date_record"
            app:layout_constraintBottom_toBottomOf="@+id/date_record" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintVertical_bias="0.53999996" android:layout_marginStart="32dp"
            android:tint="@color/colorBananna"
            android:elevation="@dimen/elevation_all"/>


</androidx.constraintlayout.widget.ConstraintLayout>