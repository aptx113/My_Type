<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
<androidx.constraintlayout.widget.ConstraintLayout
        android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent">
    <ImageButton
            android:layout_width="0dp"
            android:layout_height="40dp"
            android:id="@+id/number_water"
            android:paddingLeft="@dimen/dialog_padding"
            android:paddingRight="@dimen/dialog_padding"
            android:paddingTop="@dimen/dialog_padding"
            android:paddingBottom="@dimen/dialog_padding"
            android:textSize="@dimen/text_content"
            android:background="@drawable/button_background" android:textColor="@color/colorMyType"
            android:layout_marginStart="16dp"
            app:layout_constraintStart_toStartOf="parent" android:textAlignment="viewEnd"
            app:layout_constraintStart_toEndOf="@+id/iwater" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/number_oil" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="16dp"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/icon_water"
            android:id="@+id/iwater"
            app:layout_constraintTop_toTopOf="@+id/number_water"
            app:layout_constraintBottom_toBottomOf="@+id/number_water" android:layout_marginTop="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintEnd_toEndOf="@+id/number_water"
            android:scaleType="fitCenter"
            app:layout_constraintStart_toStartOf="@+id/number_water" android:layout_marginEnd="32dp"
            android:layout_marginStart="8dp"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@color/colorWater" android:id="@+id/line_water"
            app:layout_constraintEnd_toEndOf="@+id/number_water"
            android:layout_marginEnd="8dp" android:layout_marginStart="8dp"
            app:layout_constraintStart_toEndOf="@+id/iwater" android:layout_marginTop="16dp"
            app:layout_constraintTop_toTopOf="@+id/number_water"
            app:layout_constraintBottom_toBottomOf="@+id/number_water" android:layout_marginBottom="16dp"/>
    <ImageButton
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/number_oil"
            android:paddingLeft="@dimen/dialog_padding"
            android:paddingRight="@dimen/dialog_padding"
            android:paddingTop="@dimen/dialog_padding"
            android:paddingBottom="@dimen/dialog_padding"
            android:textSize="@dimen/text_content"
            android:background="@drawable/button_background" android:textColor="@color/colorMyType"
            android:layout_marginStart="8dp"
            app:layout_constraintStart_toEndOf="@+id/number_water"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/number_vegetable" android:textAlignment="viewEnd"
            app:layout_constraintTop_toTopOf="@+id/number_water"
            app:layout_constraintBottom_toBottomOf="@+id/number_water"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/icon_coconut"
            android:id="@+id/oil"
            app:layout_constraintTop_toTopOf="@+id/number_oil"
            app:layout_constraintBottom_toBottomOf="@+id/number_oil" android:layout_marginTop="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintEnd_toEndOf="@+id/number_oil"
            android:scaleType="fitCenter"
            app:layout_constraintStart_toStartOf="@+id/number_oil" android:layout_marginEnd="28dp"
            android:layout_marginStart="6dp"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@color/colorOil" android:id="@+id/line_oil"
            app:layout_constraintEnd_toEndOf="@+id/number_oil"
            android:layout_marginEnd="8dp" android:layout_marginStart="26dp"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toTopOf="@+id/number_oil"
            app:layout_constraintBottom_toBottomOf="@+id/number_oil" android:layout_marginBottom="16dp"
            app:layout_constraintStart_toStartOf="@+id/oil"/>
    <ImageButton
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/number_vegetable"
            android:paddingLeft="@dimen/dialog_padding"
            android:paddingRight="@dimen/dialog_padding"
            android:paddingTop="@dimen/dialog_padding"
            android:paddingBottom="@dimen/dialog_padding"
            android:textSize="@dimen/text_content"
            android:background="@drawable/button_background" android:textColor="@color/colorMyType"
            android:layout_marginStart="8dp"
            app:layout_constraintStart_toEndOf="@+id/number_oil" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/number_protein" android:textAlignment="viewEnd"
            app:layout_constraintTop_toTopOf="@+id/number_oil"
            app:layout_constraintBottom_toBottomOf="@+id/number_oil"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/icon_vegetable"
            android:id="@+id/vegetable"
            app:layout_constraintTop_toTopOf="@+id/number_vegetable"
            app:layout_constraintBottom_toBottomOf="@+id/number_vegetable" android:layout_marginTop="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintEnd_toEndOf="@+id/number_vegetable"
            android:scaleType="fitCenter"
            app:layout_constraintStart_toStartOf="@+id/number_vegetable" android:layout_marginEnd="32dp"
            android:layout_marginStart="8dp"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@color/colorVegetable" android:id="@+id/line_vegetable"
            app:layout_constraintEnd_toEndOf="@+id/number_vegetable"
            android:layout_marginEnd="8dp" android:layout_marginStart="8dp"
            app:layout_constraintStart_toEndOf="@+id/vegetable" android:layout_marginTop="16dp"
            app:layout_constraintTop_toTopOf="@+id/number_vegetable"
            app:layout_constraintBottom_toBottomOf="@+id/number_vegetable" android:layout_marginBottom="16dp"/>
    <ImageButton
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/number_protein"
            android:paddingLeft="@dimen/dialog_padding"
            android:paddingRight="@dimen/dialog_padding"
            android:paddingTop="@dimen/dialog_padding"
            android:paddingBottom="@dimen/dialog_padding"
            android:textSize="@dimen/text_content"
            android:background="@drawable/button_background" android:textColor="@color/colorMyType"
            android:layout_marginStart="8dp"
            app:layout_constraintStart_toEndOf="@+id/number_vegetable"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/number_fruit" android:textAlignment="viewEnd"
            app:layout_constraintTop_toTopOf="@+id/number_vegetable"
            app:layout_constraintBottom_toBottomOf="@+id/number_vegetable"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/icon_protein"
            android:id="@+id/protein"
            app:layout_constraintTop_toTopOf="@+id/number_protein"
            app:layout_constraintBottom_toBottomOf="@+id/number_protein" android:layout_marginTop="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintEnd_toEndOf="@+id/number_protein"
            android:scaleType="fitCenter"
            app:layout_constraintStart_toStartOf="@+id/number_protein" android:layout_marginEnd="32dp"
            android:layout_marginStart="8dp"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@color/colorProtein" android:id="@+id/line_protein"
            app:layout_constraintEnd_toEndOf="@+id/number_protein"
            android:layout_marginEnd="8dp" android:layout_marginStart="8dp"
            app:layout_constraintStart_toEndOf="@+id/protein" android:layout_marginTop="16dp"
            app:layout_constraintTop_toTopOf="@+id/number_protein"
            app:layout_constraintBottom_toBottomOf="@+id/number_protein" android:layout_marginBottom="16dp"/>
    <ImageButton
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/number_fruit"
            android:paddingLeft="@dimen/dialog_padding"
            android:paddingRight="@dimen/dialog_padding"
            android:paddingTop="@dimen/dialog_padding"
            android:paddingBottom="@dimen/dialog_padding"
            android:textSize="@dimen/text_content"
            android:background="@drawable/button_background" android:textColor="@color/colorMyType"
            android:layout_marginStart="8dp"
            app:layout_constraintStart_toEndOf="@+id/number_protein"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/number_carbon" android:textAlignment="viewEnd"
            app:layout_constraintTop_toTopOf="@+id/number_protein"
            app:layout_constraintBottom_toBottomOf="@+id/number_protein"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/icon_fruit"
            android:id="@+id/fruit"
            app:layout_constraintTop_toTopOf="@+id/number_fruit"
            app:layout_constraintBottom_toBottomOf="@+id/number_fruit" android:layout_marginTop="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintEnd_toEndOf="@+id/number_fruit"
            android:scaleType="fitCenter"
            app:layout_constraintStart_toStartOf="@+id/number_fruit" android:layout_marginEnd="32dp"
            android:layout_marginStart="8dp"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@color/colorFruit" android:id="@+id/line_fruit"
            app:layout_constraintEnd_toEndOf="@+id/number_fruit"
            android:layout_marginEnd="8dp" android:layout_marginStart="8dp"
            app:layout_constraintStart_toEndOf="@+id/fruit" android:layout_marginTop="16dp"
            app:layout_constraintTop_toTopOf="@+id/number_fruit"
            app:layout_constraintBottom_toBottomOf="@+id/number_fruit" android:layout_marginBottom="16dp"/>
    <ImageButton
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/number_carbon"
            android:paddingLeft="@dimen/dialog_padding"
            android:paddingRight="@dimen/dialog_padding"
            android:paddingTop="@dimen/dialog_padding"
            android:paddingBottom="@dimen/dialog_padding"
            android:textSize="@dimen/text_content"
            android:background="@drawable/button_background" android:textColor="@color/colorMyType"
            android:layout_marginStart="8dp"
            app:layout_constraintStart_toEndOf="@+id/number_fruit"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="16dp" android:textAlignment="viewEnd"
            app:layout_constraintTop_toTopOf="@+id/number_fruit"
            app:layout_constraintBottom_toBottomOf="@+id/number_fruit"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/icon_carbon"
            android:id="@+id/carbon"
            app:layout_constraintTop_toTopOf="@+id/number_carbon"
            app:layout_constraintBottom_toBottomOf="@+id/number_carbon"
            app:layout_constraintEnd_toEndOf="@+id/number_carbon"
            android:scaleType="fitCenter"
            app:layout_constraintStart_toStartOf="@+id/number_carbon" android:layout_marginEnd="28dp"
            android:layout_marginStart="6dp" android:layout_marginTop="8dp"
            android:layout_marginBottom="8dp"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@color/colorCarbon" android:id="@+id/line_carbon"
            app:layout_constraintEnd_toEndOf="@+id/number_carbon"
            android:layout_marginEnd="8dp" android:layout_marginStart="26dp"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toTopOf="@+id/number_carbon"
            app:layout_constraintBottom_toBottomOf="@+id/number_carbon" android:layout_marginBottom="16dp"
            app:layout_constraintStart_toStartOf="@+id/carbon"/>
    <com.terricom.mytype.linechart.LineChart
            android:id="@+id/lineChart"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:orientation="vertical"
            app:chart_padding_bottom="35dp"
            app:chart_padding_left="35dp"
            app:chart_padding_right="20dp"
            app:chart_padding_top="20dp"
            app:chart_line_color="@color/colorButton"
            app:chart_bg_color="@color/colorWhite"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginEnd="16dp" android:layout_marginStart="16dp"
            android:layout_marginBottom="16dp" android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/number_water"/>

</androidx.constraintlayout.widget.ConstraintLayout>
</layout>