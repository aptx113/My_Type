<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:id="@+id/cellDateLayout"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="72dp"
                                                   android:elevation="@dimen/elevation_all">

    <TextView
            android:id="@+id/itemDate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="3dp"
            android:layout_marginTop="2dp"
            android:text="1"
            android:textSize="12sp"
            android:textColor="@color/colorMyType"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
    <ImageView
            android:layout_width="21dp"
            android:layout_height="28dp" app:srcCompat="@drawable/icon_my_type" android:id="@+id/date_record"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@+id/date_puzzle"
            android:elevation="5dp" android:layout_marginTop="24dp"
            android:visibility="invisible" android:layout_marginStart="4dp"/>
    <ImageView
            android:layout_width="18dp"
            android:layout_height="28dp" app:srcCompat="@drawable/icon_puzzle" android:id="@+id/date_puzzle"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toStartOf="@+id/date_record"
            android:layout_marginEnd="8dp" app:layout_constraintTop_toTopOf="@+id/date_record"
            app:layout_constraintBottom_toBottomOf="@+id/date_record" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintVertical_bias="1.0"
            android:tint="@color/colorBananna"
            android:elevation="5dp" android:layout_marginStart="4dp" android:visibility="invisible"/>


</androidx.constraintlayout.widget.ConstraintLayout>