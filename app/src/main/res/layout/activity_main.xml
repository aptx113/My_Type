<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools" xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:android="http://schemas.android.com/apk/res/android">
    <data>
        <import type="com.terricom.mytype.CurrentFragmentType"/>
        <import type="android.view.View"/>
        <variable name="viewModel" type="com.terricom.mytype.MainViewModel"/>
    </data>
    <!--<androidx.drawerlayout.widget.DrawerLayout-->
            <!--android:id="@+id/drawerLayout"-->
            <!--android:layout_width="match_parent"-->
            <!--android:layout_height="match_parent"-->
            <!--android:background="@color/colorWhite">-->
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">

        <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbar"
                android:layout_width="match_parent"
                android:layout_height="80dp"
                android:minHeight="?attr/actionBarSize"
                app:layout_constraintTop_toTopOf="parent"
                android:background="@color/colorWhite"
                android:layout_gravity="center" app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                android:elevation="@dimen/elevation_all"/>

        <TextView
                android:id="@+id/text_toolbar_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/noto_sans"
                android:textColor="@color/colorMyType"
                android:letterSpacing="0.15"
                android:textSize="20sp"
                android:textStyle="bold"
                android:layout_gravity="center"
                tools:text="My Type"
                app:layout_constraintEnd_toEndOf="@+id/toolbar"
                app:layout_constraintStart_toStartOf="@+id/toolbar"
                app:layout_constraintTop_toTopOf="@+id/toolbar"
                app:layout_constraintBottom_toTopOf="@+id/myNavHostFragment"
                android:elevation="5dp"/>

        <fragment
                android:id="@+id/myNavHostFragment"
                android:name="androidx.navigation.fragment.NavHostFragment"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                app:navGraph="@navigation/navigation"
                app:defaultNavHost="true"
                app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.6"
                app:layout_constraintTop_toBottomOf="@+id/toolbar"
                app:layout_constraintBottom_toTopOf="@+id/bottom_nav_view"/>
        <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="0dp"
                android:layout_height="0dp" app:layout_constraintBottom_toBottomOf="@+id/fab"
                app:layout_constraintTop_toTopOf="@+id/fab" app:layout_constraintStart_toStartOf="@+id/fab"
                app:layout_constraintEnd_toEndOf="parent" android:id="@+id/fabLayout3" android:visibility="invisible">
            <com.google.android.material.floatingactionbutton.FloatingActionButton
                    android:id="@+id/fab3"
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_gravity="bottom|end"
                    android:layout_margin="5dp"
                    app:elevation="8dp"
                app:tint="@color/colorWhite"

                app:backgroundTint="@color/colorMyType"
                    app:srcCompat="@android:drawable/ic_menu_edit"
                    app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"/>
            <TextView
                    android:text="@string/title_sleep"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/text_sleep_title"
                    android:paddingLeft="@dimen/dialog_padding"
                    android:paddingRight="@dimen/dialog_padding"
                    android:paddingTop="@dimen/dialog_padding"
                    android:paddingBottom="@dimen/dialog_padding"
                    android:textSize="@dimen/text_content"
                    android:fontFamily="@font/noto_sans"
                    android:background="@drawable/input_column" android:textColor="@color/colorWhite"
                    app:layout_constraintStart_toEndOf="@+id/fab3"
                    app:layout_constraintTop_toTopOf="@+id/fab3" android:textAlignment="center"
                    android:layout_marginStart="8dp" app:layout_constraintBottom_toBottomOf="@+id/fab3"
            />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="wrap_content"
                android:layout_height="0dp" app:layout_constraintTop_toTopOf="@+id/fab"
                app:layout_constraintBottom_toBottomOf="@+id/fab" app:layout_constraintStart_toStartOf="@+id/fab"
                android:id="@+id/fabLayout2" android:visibility="invisible">
            <com.google.android.material.floatingactionbutton.FloatingActionButton
                    android:id="@+id/fab2"
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_gravity="bottom|end"
                    android:layout_margin="5dp"
                    app:elevation="8dp"
                    app:backgroundTint="@color/colorMyType"
                app:tint="@color/colorWhite"
                    app:srcCompat="@android:drawable/ic_menu_edit"
                    app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"/>
            <TextView
                    android:text="@string/title_shape_record"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/text_shape_record_title"
                    android:paddingLeft="@dimen/dialog_padding"
                    android:paddingRight="@dimen/dialog_padding"
                    android:paddingTop="@dimen/dialog_padding"
                    android:paddingBottom="@dimen/dialog_padding"
                    android:textSize="@dimen/text_content"
                    android:fontFamily="@font/noto_sans"
                    android:background="@drawable/input_column" android:textColor="@color/colorWhite"
                    app:layout_constraintStart_toEndOf="@+id/fab2"
                    app:layout_constraintTop_toTopOf="@+id/fab2" android:textAlignment="center"
                    android:layout_marginStart="8dp" app:layout_constraintBottom_toBottomOf="@+id/fab2"
            />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="0dp"
                android:layout_height="0dp" app:layout_constraintBottom_toBottomOf="@+id/fab"
                app:layout_constraintTop_toTopOf="@+id/fab" app:layout_constraintStart_toStartOf="@+id/fab"
                app:layout_constraintEnd_toEndOf="parent" android:id="@+id/fabLayout1" android:visibility="invisible">
            <com.google.android.material.floatingactionbutton.FloatingActionButton
                    android:id="@+id/fab1"
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_gravity="bottom|end"
                    android:layout_margin="5dp"
                    app:elevation="8dp"
                app:tint="@color/colorWhite"

                app:backgroundTint="@color/colorMyType"
                    app:srcCompat="@android:drawable/ic_menu_edit"
                    app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"/>
            <TextView
                    android:text="@string/title_foodie"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/text_foodie_title"
                    android:paddingLeft="@dimen/dialog_padding"
                    android:paddingRight="@dimen/dialog_padding"
                    android:paddingTop="@dimen/dialog_padding"
                    android:paddingBottom="@dimen/dialog_padding"
                    android:textSize="@dimen/text_content"
                    android:fontFamily="@font/noto_sans"
                    android:background="@drawable/input_column" android:textColor="@color/colorWhite"
                    app:layout_constraintStart_toEndOf="@+id/fab1"
                    app:layout_constraintTop_toTopOf="@+id/fab1" android:textAlignment="center"
                    android:layout_marginStart="8dp" app:layout_constraintBottom_toBottomOf="@+id/fab1"
            />

        </androidx.constraintlayout.widget.ConstraintLayout>
        <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/fab"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom|end"
                android:layout_alignParentBottom="false"
                android:layout_alignParentRight="false"
                app:elevation="10dp"
                app:backgroundTint="@color/colorWhite"
                app:srcCompat="@android:drawable/ic_input_add"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintHorizontal_bias="0.487"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginBottom="16dp"/>

        <com.google.android.material.bottomnavigation.BottomNavigationView
                android:id="@+id/bottom_nav_view"
                android:layout_width="0dp"
                android:layout_height="@dimen/height_bottom_nav_view_main"
                app:itemIconSize="@dimen/size_bottom_nav_icon"
                android:background="@color/colorMyType"
                app:itemIconTint="@color/btm_navigation_selector"
                app:itemTextColor="@color/btm_navigation_selector"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:labelVisibilityMode="labeled"
                app:menu="@menu/bottom_nav_menu">

        </com.google.android.material.bottomnavigation.BottomNavigationView>


    </androidx.constraintlayout.widget.ConstraintLayout>


    <!--</androidx.drawerlayout.widget.DrawerLayout>-->

</layout>